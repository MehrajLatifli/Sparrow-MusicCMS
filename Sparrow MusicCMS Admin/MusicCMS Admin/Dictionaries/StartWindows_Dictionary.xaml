<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=netstandard">

    <SolidColorBrush x:Key="ProgressBarForeGroundColor">#02be68</SolidColorBrush>

    <SolidColorBrush x:Key="StartViewFontColor">#02be68</SolidColorBrush>

    <sys:Double x:Key="StartViewFontSize">35</sys:Double>

    <SolidColorBrush x:Key="ProgressBarBackGroundColor" Color="White" />

    <Style x:Key="CooperBlackFont">
        <Setter Property="TextElement.FontFamily" Value="/Fonts/#Cooper Black" />
    </Style>

    <SolidColorBrush x:Key="StartFontForeGroundColor">#02be68</SolidColorBrush>

</ResourceDictionary>